{
	"extends": ["eslint:recommended",
							"plugin:compat/recommended"],
	"env": {
	  "browser": true,
	  "es2020": true // es2020
	},
    "parserOptions": {
      "sourceType": "module"
  },
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["warn", "single"],
		"no-unused-vars": ["warn"],
		"no-unreachable": ["warn"],
		"no-var": ["error"],
		"prefer-const": ["warn"]
	}
}